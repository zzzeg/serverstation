
page{
    background: #EFF1F6;
}
.hintText{
    font-size:28rpx;
    font-family:NotoSansCJKsc-Regular,NotoSansCJKsc;
    font-weight:400;
    color:rgba(2,172,130,1);
    line-height:40rpx;
    padding: 30rpx;
}
.plan-box{
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
            flex-direction: row;
    background: #FFFFFF;
    font-size:29rpx;
    font-family:NotoSansCJKsc-Regular,NotoSansCJKsc;
    font-weight:400;
    color:rgba(67,78,94,1);
    border-radius:30rpx;
    margin: 0rpx 20rpx 40rpx 20rpx;
}
.plan-box-title{
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
            flex-direction: row;
    margin-left: 33rpx;
    margin-right: 33rpx;
    margin-top: 30rpx;
    margin-bottom: 30rpx;
    width: calc(100% - 66rpx);
}
.plan-box image{
    width: 44rpx;
    height: 44rpx;
}
.toolBar image{
    width: 44rpx;
    height: 44rpx;
}
.plan-box-title image{
    width: 156rpx;
    height: 142rpx;
    border-radius:10rpx;
}
.plan-box-name{
    margin-left: 12rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
            flex-direction: column;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
}
.moneyBox{
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
            flex-direction: row;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
}
.remarksBox{
    display: -webkit-box;
